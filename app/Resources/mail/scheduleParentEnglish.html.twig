    <div>&nbsp;</div>
    <h2>Virtual lessons:</h2>
    <div>&nbsp;</div>

    {% for studentLesson in studentLessons %}
       <div>&nbsp;</div>
       <h3>{{studentLesson.student.englishName}} ({{studentLesson.student.firstName}} {{studentLesson.student.lastName}})</h3>
       <div>&nbsp;</div>
       <table class="table">
        <thead>
          <tr>
            <th>Start</th>
            <th>Subject</th>
            <th>WebLink</th>
            <th>Password</th>
            <th>Teacher</th>
            <th>Phone</th>
          </tr>
        </thead>
        <tbody>
         <tr>
            <td class="high">09:10</td>
            <td class="high">Common meeting and service</td>
            <td class="high"><a href="https://us04web.zoom.us/j/8626417376?pwd=U3oxV3ZjT0xnL3M0cHl5MzJDeGdsZz09">https://us04web.zoom.us/j/8626417376?pwd=U3oxV3ZjT0xnL3M0cHl5MzJDeGdsZz09</a></td>
            <td class="pass"></td>
            <td class="high">Pavel Molchanov</td>
            <td class="high"><a href="tel:8455530997">(845)-553-0997</a></td>
          </tr>
          {% for lesson in studentLesson.lessons %}
              <tr style="padding-bottom: 5px">
                <td>{{ lesson.period.startTime|date("H:i") }}</td>
                <td>{{ lesson.course.englishName }}</td>
                <td><a href="{{ lesson.teacher.meetingLink }}">{{ lesson.teacher.meetingLink }}</a></td>
                <td class="pass">{{ lesson.teacher.meetingPassword }}</td>
                {% if lesson.teacher.englishName is not empty %}
                  <td>{{ lesson.teacher.englishName }}</td>
                {% else %}
                  <td>{{ lesson.teacher.firstName }} {{ lesson.teacher.lastName }}</td>
                {% endif %}
                <td  style="white-space: nowrap"><a href="tel:{{ lesson.teacher.mobilePhone }}">{{ lesson.teacher.mobilePhone }}</a></td>
              </tr>
          {% endfor %}
        </tbody>
      </table>
    {% endfor %}