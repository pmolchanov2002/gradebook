{# app/Resources/views/teacher/exam.html.twig #}
{% extends 'base-teacher.html.twig' %}

{% block title %}Quater or final grades{% endblock %}

{% block body %}
<div class="container">
  <!-- Example row of columns -->
  <a class="btn btn-default" href="/teacher" role="button">&lt;&lt; Back</a>
  <h3>Select period:</h3>
  {%for exam in exams %}
  <div class="row">
      <div class="col-md-10">
        <p><a class="btn btn-primary btn-lg" href="/teacher/exam/{{ exam.id }}/course" role="button">{{ exam.name }} &raquo;</a></p>
      </div>
  </div>
  {% endfor %}
</div>
{% endblock %} 